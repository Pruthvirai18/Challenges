#Code 1
//To find the greatest of the two numbers using if-else
#include<stdio.h>
void main()
{
    int a,b;
    printf("Enter the value for a and b ");
    scanf("%d %d",&a,&b);
    if(a==b)
    {
        printf("Both %d and %d are equal",a,b);
    }
    else if(a>b)
    {
        printf("%d is greater than %d",a,b);
    }
    else 
    {
        printf("%d is lesser than %d",a,b);
    }
}
